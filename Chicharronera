public class Chicharronera
{
  public static void main(String[] args)
  {
    double a, b, c;
    double chicharronera1, chicharronera2;

    a= Double.parseDouble (args[0]);
    b= Double.parseDouble (args[1]);
    c= Double.parseDouble (args[2]);

    chicharronera1 =(-b + Math.sqrt (b*b -4*a*c)) / (2*a) ;
    chicharronera2 =(-b - Math.sqrt (b*b -4*a*c)) / (2*a) ;

    System.out.println ("La chicharronera es "+ chicharronera1);
    System.out.println ("y "+ chicharronera2);
  }
}
